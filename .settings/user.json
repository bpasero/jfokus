{
    "type": "user",
    "editor": {
        "lineNumbers": true,
        "glyphMargin": false,
        "tabSize": "auto",
        "insertSpaces": "auto",
        "roundedSelection": true,
        "scrollBeyondLastLine": true,
        "wrappingColumn": 300,
        "quickSuggestions": true,
        "quickSuggestionsDelay": 500,
        "autoClosingBrackets": true,
        "formatOnType": false,
        "suggestOnTriggerCharacters": true,
        "overviewRulerLanes": 3,
        "hideCursorInOverviewRuler": false
    },
    "console": {
        "lineNumbers": false,
        "roundedSelection": true,
        "wrappingColumn": 300,
        "quickSuggestionsDelay": 50,
        "scrollBeyondLastLine": true,
        "hideCursorInOverviewRuler": true
    },
    "languages": {
        "vs.languages.javascript": {
            "suggestSettings": {
                "alwaysAllWords": false,
                "useCodeSnippetsOnMethodSuggest": false
            },
            "validationSettings": {
                "target": "ES5",
                "module": "",
                "baseUrl": "",
                "lint": {
                    "curlyBracketsMustNotBeOmitted": "ignore",
                    "emptyBlocksWithoutComment": "ignore",
                    "comparisonOperatorsNotStrict": "ignore",
                    "missingSemicolon": "ignore",
                    "reservedKeywords": "warning",
                    "typeScriptSpecifics": "warning",
                    "unknownTypeOfResults": "warning",
                    "semicolonsInsteadOfBlocks": "ignore",
                    "functionsInsideLoops": "ignore",
                    "tripleSlashReferenceAlike": "warning",
                    "unusedVariables": "ignore",
                    "unusedFunctions": "ignore",
                    "newOnLowercaseFunctions": "warning",
                    "newOnReturningFunctions": "warning",
                    "redeclaredVariables": "warning",
                    "undeclaredVariables": "error",
                    "unknownProperty": "ignore",
                    "primitivesInInstanceOf": "error",
                    "mixedTypesArithmetics": "warning",
                    "forcedTypeConversion": "warning"
                }
            }
        },
        "vs.languages.json": {
            "schemas": []
        },
        "vs.languages.css": {
            "box-model": "ignore",
            "compatible-vendor-prefixes": "warning",
            "display-property-grouping": "warning",
            "duplicate-background-images": "ignore",
            "duplicate-properties": "warning",
            "empty-rules": "warning",
            "gradients": "warning",
            "ids": "ignore",
            "important": "ignore",
            "known-properties": "ignore",
            "outline-none": "warning",
            "overqualified-elements": "ignore",
            "qualified-headings": "ignore",
            "regex-selectors": "ignore",
            "shorthand": "ignore",
            "text-indent": "ignore",
            "unique-headings": "ignore",
            "universal-selector": "ignore",
            "unqualified-attributes": "ignore",
            "vendor-prefix": "warning",
            "zero-units": "warning"
        },
        "vs.languages.typescript": {
            "suggestSettings": {
                "alwaysAllWords": false,
                "useCodeSnippetsOnMethodSuggest": false
            },
            "allowMultipleWorkers": false,
            "validationSettings": {
                "target": "ES5",
                "baseUrl": "",
                "module": "",
                "lint": {
                    "emptyBlocksWithoutComment": "ignore",
                    "curlyBracketsMustNotBeOmitted": "ignore",
                    "comparisonOperatorsNotStrict": "ignore",
                    "missingSemicolon": "ignore",
                    "unknownTypeOfResults": "warning",
                    "semicolonsInsteadOfBlocks": "ignore",
                    "functionsInsideLoops": "ignore",
                    "functionsWithoutReturnType": "warning",
                    "tripleSlashReferenceAlike": "warning",
                    "unusedImports": "ignore",
                    "unusedVariables": "ignore",
                    "unusedFunctions": "ignore",
                    "unusedMembers": "ignore"
                }
            }
        }
    },
    "run": {
        "suffix": ""
    },
    "search": {
        "excludeFolders": [
            ".git",
            "node_modules"
        ]
    },
    "theme": "vs"
}